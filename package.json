{
    "name": "glesys-api",
    "version": "8.5.0",
    "description": "Node.js wrapper for the GleSYS API.",
    "type": "module",
    "main": "./lib/glesys.js",
    "exports": "./lib/glesys.js",
    "author": "Jonathan Wilsson <jonathan.wilsson@gmail.com>",
    "contributors": [
        "Oskar Risberg <kokarn@gmail.com>"
    ],
    "engines": {
        "node": ">=18.12.0"
    },
    "keywords": [
        "glesys"
    ],
    "files": [
        "lib"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/jwilsson/glesys-api-node"
    },
    "dependencies": {
        "got": "^13.0.0"
    },
    "devDependencies": {
        "@jest/globals": "^29.0.1",
        "@jwilsson/eslint-config": "^9.0.0",
        "@jwilsson/prettier-config": "^9.0.0",
        "coveralls": "^3.0.0",
        "eslint": "^9.10.0",
        "jest": "^29.0.1",
        "prettier": "^3.3.3",
        "simple-git-hooks": "^2.2.0"
    },
    "scripts": {
        "pretest": "eslint lib test",
        "test": "node --experimental-vm-modules node_modules/.bin/jest"
    },
    "simple-git-hooks": {
        "pre-commit": "npx lint-staged"
    },
    "lint-staged": {
        "*.js": "prettier --write lib test"
    },
    "jest": {
        "collectCoverage": true,
        "setupFilesAfterEnv": [
            "<rootDir>/jestSetup.js"
        ],
        "testMatch": [
            "<rootDir>/test/**/*.js"
        ],
        "verbose": true
    },
    "prettier": "@jwilsson/prettier-config",
    "license": "MIT"
}

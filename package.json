{
  "name": "glesys-api",
  "version": "5.3.0",
  "description": "Node.js wrapper for the GleSYS API.",
  "main": "lib/glesys.js",
  "author": "Jonathan Wilsson <jonathan.wilsson@gmail.com>",
  "contributors": [
    "Oskar Risberg <kokarn@gmail.com>"
  ],
  "engines": {
    "node": ">=14.15.0"
  },
  "keywords": [
    "glesys"
  ],
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jwilsson/glesys-api-node"
  },
  "exports": {
    ".": {
      "require": "./lib/glesys.js",
      "import": "./lib/glesys.mjs"
    }
  },
  "dependencies": {
    "got": "^10.0.1"
  },
  "devDependencies": {
    "@jwilsson/eslint-config-node": "^6.0.0",
    "@jwilsson/eslint-config-prettier": "^1.0.0",
    "@jwilsson/prettier-config": "^1.0.0",
    "coveralls": "^3.0.0",
    "eslint": "^7.15.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "simple-git-hooks": "^2.2.0"
  },
  "scripts": {
    "pretest": "eslint lib test",
    "test": "jest "
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.js": "prettier --write lib test"
  },
  "babel": {},
  "eslintConfig": {
    "extends": [
      "@jwilsson/eslint-config-node",
      "@jwilsson/eslint-config-prettier"
    ],
    "env": {
      "jest": true
    }
  },
  "jest": {
    "collectCoverage": true,
    "setupFilesAfterEnv": [
      "<rootDir>/jestSetup.js"
    ],
    "testMatch": [
      "<rootDir>/test/**/*.js"
    ],
    "verbose": true
  },
  "prettier": "@jwilsson/prettier-config",
  "license": "MIT"
}
